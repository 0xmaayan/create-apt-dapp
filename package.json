  ],
  "type": "module",
  "dependencies": {
    "commander": "^10.0.1",
    "prompts": "^2.4.2",
    "typescript": "^4.7.4",
    "kolorist": "^1.8.0"
  },
  "devDependencies": {
    "@types/node": "^20.5.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aptos-labs/create-aptos-dapp"
  },
  "homepage": "https://github.com/aptos-labs/create-aptos-dapp/blob/main/README.md"
}
