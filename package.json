{
  "name": "create-apt-dapp",
  "version": "0.0.27",
  "keywords": [
    "create aptos dapp",
    "create-aptos-dapp",
    "create-apt-dapp",
    "create apt dapp",
    "aptos",
    "boilerplate"
  ],
  "bin": {
    "create-apt-dapp": "./dist/index.js"
  },
